{
  "name": "cursor-balance",
  "displayName": "Cursor Balance",
  "description": "查看Cursor API使用情况及剩余配额",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxmeng93/cursor-balance"
  },
  "activationEvents": [
    "onCommand:cursor-balance.showBalance"
  ],
  "contributes": {
    "commands": [
      {
        "command": "cursor-balance.showBalance",
        "title": "查看Cursor API使用情况"
      }
    ],
    "configuration": {
      "title": "Cursor Balance",
      "properties": {
        "cursorBalance.token": {
          "type": "string",
          "default": "",
          "description": "WorkosCursorSessionToken"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "npx @vscode/vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.0",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
